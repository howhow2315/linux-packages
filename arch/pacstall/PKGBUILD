pkgname=pacstall
pkgver=1.0.2
pkgrel=1
pkgdesc="A simple pacman wrapper for shorthand inside scripts and basic usage"
arch=('any')
url="https://github.com/howhow2315/linux-packages"
license=('MIT')
depends=('pacman')
source=("pacstall.sh")
sha256sums=('e9baededb52db5767d16d59e97f22993768853503f398f6605ccf2255cf4b514')

package() {
  install -Dm755 pacstall.sh "$pkgdir/usr/bin/pacstall"
}
